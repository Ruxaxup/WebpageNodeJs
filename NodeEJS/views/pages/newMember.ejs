<html lang="en">

<head>
  <% include ../partials/head %>
</head>

<body class="container">
  <header>
    <% include ../partials/header %>
  </header>

  <main>
    <div class="container">
      <div class="col-sm-6 col-sm-offset-3">
        <form action="/registerMember" method="post" enctype="application/x-www-form-urlencoded">
          <div class="form-group">
            <label for="pwd">Membres&iacute;a:</label>
            <input type="text" onkeypress='return event.charCode >= 48 && event.charCode <= 57' class="text-uppercase form-control" name="membershipID" />
            <label for="pwd">Nombre(s):</label>
            <input type="text" class="text-uppercase form-control" id="name" name="name" />
            <label for="pwd">Primer Apellido:</label>
            <input type="text" class="text-uppercase form-control" name="dadLastName" />
            <label for="pwd">Segundo Apellido:</label>
            <input type="text" class="text-uppercase form-control" name="momLastName" />

          </div>
          <button type="submit" class="btn btn-default">Registrar</button>
        </form>
      </div>
    </div>
  </main>
  <footer>
    <% include ../partials/footer %>
  </footer>
</body>

</html>
